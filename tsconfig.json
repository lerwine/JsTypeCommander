{
    "compilerOptions": {
        "target":"es5",
        "module":"commonjs",
        "strictNullChecks": true,
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "lib": [
            "es5"
        ], 
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "./src/**/test-*.ts",
        "./src/**/testLib.ts"
    ]
}