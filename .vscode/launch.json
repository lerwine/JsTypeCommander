{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "-u", "tdd",
                "-d",
                "--timeout", "999999",
                "--colors",
                "--recursive",
                "-r", "ts-node/register",
                "--reporter", "spec",
                "./test/**/.test-*js"
            ],
            "cwd": "${workspaceFolder}",
            "sourceMaps": true,
            "preLaunchTask": "buildAll",
            "internalConsoleOptions": "openOnSessionStart",
            "protocol": "inspector"
